<UserControl
    x:Class="SelectByParameters.UI.Values.StrinListControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:SelectByParameters.UI.Values"
    mc:Ignorable="d"
    d:DataContext="{d:DesignInstance local:StringListVm}">
    <ComboBox ItemsSource="{Binding Values}" SelectedItem="{Binding Value}"/>
</UserControl>
